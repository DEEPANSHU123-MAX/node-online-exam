<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <table  style="border:1px solid black;" class="table">
      
        <h2>View results</h2>
       
        <tr >
            <th>S.no</th>
            <th>Names</th>
            <th>Email</th>
            <th>Exam name</th>
            <th>Grade</th>
            <th>Duration</th>
            <th>status</th>
            
        </tr>
        <tbody>
    <% users.forEach(function (user , index){ %>
        <tr>
            <td><%= index + 1 %></td>
            <td><%= user.name %> </td>
            <td><%= user.email %> </td>
            <td> <% if( user.results.length!=0){ %>
                <%  user.results.forEach(exam=>{ %>
                    <%= exam.exam_name %><br>
                    <% }) %>
              <% } else{ %>  
                <h4>not attempted</h4>
             <% } %></td>
            <td> <% if( user.results.length!=0){ %>
                <%  user.results.forEach(exam=>{ %>
                    <%= exam.finalScore %>/ <%= exam.totalQuestions %><br>
                    <% }) %>
              <% } else{ %>  
                <h4>NULL</h4>
             <% } %></td>
             <td> <% if( user.results.length!=0){ %>
                <%  user.results.forEach(exam=>{ %>
                    <%= exam.totalMinutesTaken %> min<br>
                    <% }) %>
              <% } else{ %>  
                <h4>NULL</h4>
             <% } %></td>
             <td> <% if( user.results.length!=0){ %>
                <%  user.results.forEach(exam=>{ %>
                    âœ” <br>
                    <% }) %>
              <% } else{ %>  
                <h4>NULL</h4>
             <% } %></td>
  
            <% }) %>
           
       
         
       
           
        </tr>
  
       
        <tbody>
</body>
</html>